# =============================================================================
# API Keys (Optional - leave empty to require user input on frontend)
# =============================================================================

# MorphLLM API Key
# Get yours at: https://morphllm.com/dashboard
NEXT_PUBLIC_MORPHLLM_API_KEY=

# Relace API Key
# Get yours at: https://app.relace.ai/
NEXT_PUBLIC_RELACE_API_KEY=

# =============================================================================
# API Endpoints (Optional - leave empty to use provider defaults)
# =============================================================================

# MorphLLM Custom Endpoint
# Default: https://api.morphllm.com/v1/chat/completions
NEXT_PUBLIC_MORPHLLM_ENDPOINT=

# Relace Custom Endpoint
# Default: https://api.relace.ai/api/instant-apply
NEXT_PUBLIC_RELACE_ENDPOINT=

# =============================================================================
# Default Provider Settings
# =============================================================================

# Which provider to pre-select on page load
# Options: morphllm | relace
NEXT_PUBLIC_DEFAULT_PROVIDER=morphllm

# =============================================================================
# Default Models (Pre-selected on page load - users can change in UI)
# =============================================================================

# MorphLLM Default Model
# Options: morph-v3-fast | morph-v3-large | auto
NEXT_PUBLIC_MORPHLLM_DEFAULT_MODEL=morph-v3-large

# Relace Default Model
# Options: auto | relace-apply-2.5-lite | relace-apply-2
NEXT_PUBLIC_RELACE_DEFAULT_MODEL=auto

# =============================================================================
# Rate Limiting
# =============================================================================

# Enable or disable rate limiting
NEXT_PUBLIC_ENABLE_RATE_LIMIT=false

# Rate limit period
# Options: MONTH | DAY
NEXT_PUBLIC_RATE_LIMIT_PERIOD=MONTH

# Maximum requests per month (only applies if RATE_LIMIT_PERIOD=MONTH)
NEXT_PUBLIC_MAX_REQUESTS_PER_MONTH=100

# Maximum requests per day (only applies if RATE_LIMIT_PERIOD=DAY)
NEXT_PUBLIC_MAX_REQUESTS_PER_DAY=10
